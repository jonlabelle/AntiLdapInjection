root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
tab_width = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.sh]
end_of_line = lf

[*.{yml,json,xml,csproj,targets,props}]
indent_size = 2

[*.{sln,bat,cmd}]
end_of_line = crlf
indent_style = tab
